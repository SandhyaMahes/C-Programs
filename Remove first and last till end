#include<stdio.h>
#include <stdlib.h>

int main()
{
    char s[1000];
    int n,i,j,k=0;
    scanf("%s",s);
    n=strlen(s);
    if(n==1)
    printf("%s",s);
    for(i=1;s[i]!='\0';i++)
    {
       for(j=k;j<n;j++)
       {
           printf("%c",s[j]);
       }
       k++;
       n=n-1;
       printf("\n");
       
    }

}