#include<stdio.h>
#include <stdlib.h>

int main()
{
    char s1[100],s2[100];
    int i=0,j=1;
    scanf("%s%s",s1,s2);
    for(;s1[i]!='\0';i++)
    {
        printf("%c",s1[i]);
        if(s1[i]==s2[0] && j)
        {
            printf("\%s",&s2[1]);
            j=0;
        }
    
        printf("\n");
    }
    
    


}