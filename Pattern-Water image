#include<stdio.h>
#include <stdlib.h>

int main()
{
    int n,i,j,count;
    scanf("%d",&n);
    count=n;
    for(i=0;i<n;i++)
    {
       for(j=count;j>0;j--)
       {
           printf("%d",count);
       }
       printf("\n");
       count--;
    }
    count=2;
    for(i=2;i<=n;i++)
    {
        for(j=1;j<=count;j++)
        {
            printf("%d",count);
        }
        printf("\n");
        count++;
    }
}

INPUT:
5

OUTPUT:
55555
4444
333
22
1
22
333
4444
55555