#include<stdio.h>
#include <stdlib.h>

int main()
{
    char s1[100],s2[100];
    int i,j=1,k,l1,l2;
    scanf("%s%s",s1,s2);
    l1=strlen(s1);
    l2=strlen(s2);
    for(i=0;i<l1;i++)
    {
        printf("%c",s1[i]);
        if(s1[i]==s2[l2-1]&& j)
        {
            for(k=l2-2;k>=0;k--)
            {
                printf("%c",s2[k]);
            }
            j=0;
            
        }
        printf("\n");
    }
}