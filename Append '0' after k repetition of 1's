#include<stdio.h>
#include <stdlib.h>

int main()
{
    int n,k,i,count=0;
    scanf("%d %d",&n,&k);
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    for(i=0;i<n;i++)
    {
        if(a[i]==1)
        {
            count++;
        }
        else
        {
            count=0;
        }
        if(count==k)
        {
            printf("%d 0",a[i]);
            count=0;
        }
        else
        {
            printf("%d",a[i]);
        }
    }
}